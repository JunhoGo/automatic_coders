package guitest2;import java.util.Timer;import java.util.TimerTask;import java.util.Date;import javax.imageio.ImageIO;import javax.swing.JFrame;import javax.swing.JPanel;import java.awt.Dimension;import java.awt.Graphics;import java.awt.Panel;import java.awt.image.BufferedImage;import java.io.File;import java.io.IOException;public class NewPrintTimer extends JFrame {  BufferedImage img=null; public void MyImageFrame(){  setTitle("Image Load test");     }  public static void main(String[] args) {  // TODO Auto-generated method stub  NewScheduledJob job = new NewScheduledJob();//        Timer jobScheduler = new Timer(true); //        jobScheduler.scheduleAtFixedRate(job, 1000, 1500);//        try {           Thread.sleep(10000);        } catch(InterruptedException ex) {           //        }        // cancel() 메소드를 호출하지 않는다.     } } class NewScheduledJob extends TimerTask{  int index=1;  BufferedImage img=null;    public void run() {     setTitle("Image Load Test");                  switch(index)       {         case 1:         {          try {           System.out.println(index+"번째 실행");           img=ImageIO.read(new File("train1.png"));           Thread.sleep(1250);          } catch (IOException e) {           // TODO Auto-generated catch block           System.out.println(e.getMessage());           System.exit(0);           e.printStackTrace();          } catch (InterruptedException e) {     // TODO Auto-generated catch block     e.printStackTrace();    }                    add(new Panel());          pack();          setVisible(true);          index++;        }         case 2:         {          try {           System.out.println(index+"번째 실행");           img=ImageIO.read(new File("train2.png"));           Thread.sleep(1250);          } catch (IOException e) {           // TODO Auto-generated catch block           System.out.println(e.getMessage());           System.exit(0);           e.printStackTrace();          } catch (InterruptedException e) {     // TODO Aut